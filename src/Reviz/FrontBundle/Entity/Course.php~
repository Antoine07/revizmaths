<?php

namespace Reviz\FrontBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Course
 *
 * @ORM\Entity
 * @ORM\Table(name="posts")
 */
class Course extends Post
{
    private $course;

    public function getCourse() {

        return $this->course;
    }
}
